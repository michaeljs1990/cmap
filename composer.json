{
    "name": "michaeljs1990/composer-map",
    "description": "Composer check package dependencies",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Schuett",
            "email": "michaeljs1990@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "~2.8",
        "guzzlehttp/guzzle": "~6"
    },
    "autoload": {
        "psr-4": { "Michaeljs1990\\Cmap\\": "src" }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload-dev": {
      "psr-4": { "Michaeljs1990\\Cmap\\Test\\": "test" }
    },
    "bin": [
        "cmap"
    ],
    "minimum-stability": "dev"
}
